{% block neutron_autocomplete_widget %}
{% spaceless %}
<div>
    {{ form_widget(form) }}
    <span class="neutron-autocomplete" style="display:none" data-options="{{ configs|merge({'id': id})|json_encode }}"></span>
</div>
{% endspaceless %}
{% endblock neutron_autocomplete_widget %}

{% block neutron_spinner_widget %}
{% spaceless %}
<div>
    {{ form_widget(form) }}
    <span class="neutron-spinner" style="display:none" data-options="{{ configs|merge({'id': id, 'culture': app.request.locale })|json_encode }}"></span>
</div>
{% endspaceless %}
{% endblock neutron_spinner_widget %}

{% block neutron_buttonset_widget %} 
