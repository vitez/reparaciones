{% block neutron_image_upload_widget %}
{% spaceless %}
<div>    
    {{ form_widget(form) }} 
    <div class="neutron-image-upload-main" style="display:none">
        {{ block('neutron_image_upload_container') }} 
        {{ block('neutron_image_upload_crop') }}
        {{ block('neutron_upload_meta') }}
    </div>
    
    <span class="neutron-image-upload" data-options="{{ configs|merge({'base_url': asset('')})|json_encode }}" style="display:none"></span>
</div>
{% endspaceless %}
{% endblock neutron_image_upload_widget %}

{% block neutron_image_upload_container %}
{% spaceless %}
<div id="neutron-image-upload-container-{{ id }}">
    <div class="ui-state-error" style="width:300px; padding:5px; display:none">
        <span style="float:right">
            <a href="#close" id="neutron-upload-error-cancel-{{ id }}">
                <img src="{{ asset('bundles/neutronform/images/cancel.png') }}" />
            </a>
        </span>
        <p>
            <span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span> 
            <span class="neutron-imageupload-error">&nbsp</span>
        </p>
    </div>
   
    <div  id="neutron-progressbar-{{ id }}" style="width:350px; height:10px; display: none" ></div>
    
    <div class="neutron-progress-info" style="display:none">
        <div class="ui-widget">
            <div class="ui-state-highlight ui-corner-all" style="width:340px;padding:5px;"> 
                <span style="float:right" id="neutron-upload-remove-image-{{id}}">
                    <a href="#remove">
                        <img src="{{ asset('bundles/neutronform/images/cancel.png') }}" />
                    </a>
                </span>
                <span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                <span>{% trans from translation_domain %}upload_progress{% endtrans %}: <strong>0%</strong></span>
                <br/>
                <span id="neutron-image-info-{{ id }}">__image_name__ (__image_size__)</span>
            </div>
        </div>
    </div>
        
    <div class="neutron-image">
        <img id="neutron-image-{{ id }}" src="{{ asset('bundles/neutronform/images/noImage.png') }}" />
    </div>
    
    {{ block('neutron_image_controls') }}
</div>

{% endspaceless %}
{% endblock neutron_image_upload_container %}

{% block neutron_image_controls %}
{% spaceless %}
<div class="neutron-controls">
    <button id="neutron-image-btn-upload-{{ id }}" class="ui-icon ui-icon-newwin neutron-image-upload-button" 
        title="{% trans from translation_domain %}button_upload_title{% endtrans %}"></button> &nbsp;
    {% if configs.image_options.enabled_button %}
        <a id="neutron-image-btn-enabled-{{ id }}" 
            class="ui-icon {% if (configs.enabled_value)  %} ui-icon-bullet  {% else %} ui-icon-radio-on {% endif %} neutron-image-upload-button" 
            title="{% trans from translation_domain %}button_title_enabled{% endtrans %}"></a>  &nbsp;
    {% endif %}
    {% if configs.image_options.view_button %}
        <a id="neutron-image-btn-view-{{ id }}" class="ui-icon ui-icon-zoomin neutron-image-upload-button" 
            title="{% trans from translation_domain %}button_view_title{% endtrans %}"></a>  &nbsp;
    {% endif %}
    {% if configs.image_options.crop_button %}
        <a id="neutron-image-btn-crop-{{ id }}" class="ui-icon ui-icon-scissors neutron-image-upload-button" 
            title="{% trans from translation_domain %}button_crop_title{% endtrans %}"></a>  &nbsp;
    {% endif %}
    {% if configs.image_options.meta_button %}
        <a id="neutron-meta-btn-edit-{{ id }}" class="ui-icon ui-icon-pencil neutron-image-upload-button" 
            title="{% trans from translation_domain %}button_edit_meta_title{% endtrans %}"></a>  &nbsp;
    {% endif %}  
    {% if configs.image_options.rotate_button %}
        <a id="neutron-image-btn-rotate-{{ id }}" class="ui-icon ui-icon-arrowrefresh-1-s neutron-image-upload-button" 
            title="{% trans from translation_domain %}button_rotate_title{% endtrans %}"></a> &nbsp;
    {% endif %}
    {% if configs.image_options.reset_button %}
        <a id="neutron-image-btn-reset-{{ id }}" class="ui-icon ui-icon-cancel neutron-image-upload-button" 
            title="{% trans from translation_domain %}button_reset_title{% endtrans %}"></a>  &nbsp;
    {% endif %} 
    <a id="neutron-image-btn-remove-{{ id }}" class="ui-icon ui-icon-trash neutron-image-upload-button" 
        title="{% trans from translation_domain %}button_remove_title{% endtrans %}"></a>  &nbsp;
</div>
{% endspaceless %}
{% endblock neutron_image_controls %}

{% block neutron_image_upload_crop %}
{% spaceless %}
<div id="neutron-dlg-image-crop-{{ id }}" style="display:none" 
    title="{% trans from translation_domain %}dialog_crop_title{% endtrans %}">
   
    <div id="neutron-image-crop-{{ id }}" class="neutron-image"></div>

    <div style="float:right">
        <input type="hidden" id="x-{{ id }}"/>
        <input type="hidden" id="y-{{ id }}"/>
        <input type="hidden" id="w-{{ id }}"/>
        <input type="hidden" id="h-{{ id }}"/>
        
        <a id="neutron-crop-dlg-cancel-btn-{{ id }}" class="neutron-image-upload-button">
            {% trans from translation_domain %}dialog_crop_button_cancel{% endtrans %}</a>
        <a id="neutron-crop-dlg-save-btn-{{ id }}" class="neutron-image-upload-button">
            {% trans from translation_domain %}dialog_crop_button_crop{% endtrans %}</a>  &nbsp;    
    </div>
</div>  
{% endspaceless %}
{% endblock neutron_image_upload_crop %}

{% block neutron_upload_meta %}
{% spaceless %}
<div id="neutron-dlg-meta-edit-{{ id }}" class="neutron-form" 
    title="{% trans from translation_domain %}dialog_edit_meta_title{% endtrans %}">
   <div>
       <label for="neutron_meta_title-{{ id }}" class="neutron-form-label">
           {% trans from translation_domain %}dialog_edit_meta_label_title{% endtrans %}
       </label>
       <input type="text" id="neutron-meta-title-{{ id }}" class="neutron-form-text" />
   </div>
   
   <div>
       <label for="neutron-meta-caption-{{ id }}" class="neutron-form-label" >
           {% trans from translation_domain %}dialog_edit_meta_label_caption{% endtrans %}
       </label>
       <input type="text" id="neutron-meta-caption-{{ id }}" class="neutron-form-text" />
   </div>
   
   <div>
       <label for="neutron-meta-description-{{ id }}" class="neutron-form-label">
           {% trans from translation_domain %}dialog_edit_meta_label_description{% endtrans %}
       </label>
       <textarea id="neutron-meta-description-{{ id }}" class="neutron-form-text" rows="5" ></textarea>
   </div>
   
   <div style="float:right">
       <a id="neutron-edit-dlg-done-btn-{{ id }}" class="neutron-image-upload-button">
           {% trans from translation_domain %}dialog_edit_meta_button_done{% endtrans %}
       </a>
   </div>
   
</div>
{% endspaceless %}
{% endblock neutron_upload_meta %}

{% block neutron_multi_image_upload_collection_widget %}
{% spaceless %}
<div>
    <div class="neutron-multi-image-upload-main" style="display:none">
        <div id="neutron-multi-image-upload-error-{{ id }}" 
            class="neutron-multi-image-upload-error" style="display:none">
    
            <div class="ui-state-error" style="width:340px; padding:5px;">         
                <span style="float:right">
                    <a href="#close" id="neutron-multi-upload-error-cancel-{{ id }}">
                        <img src="{{ asset('bundles/neutronform/images/cancel.png') }}" />
                    </a>
                </span>
    
                <span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span> 
                <span class="neutron-imageupload-error">&nbsp;</span>       
            </div>
        </div>
    	
        <div id="multi-image-progress-{{ id }}"></div>
    
        <div id="drag-drop-area-{{ id }}" class="neutron-drag-drop" {% if form|length > 0 %}style="display:none;"{% endif %}>
            <div class="neutron-drag-drop-inside">
                <p class="neutron-drag-drop-info" id="neutron-drag-drop-info-{{ id }}" 
                    trans-drag="{% trans from translation_domain %}drag_drop_image_text{% endtrans %}"
                    trans-no-images="{% trans from translation_domain %}no_images_text{% endtrans %}"
                ></p>
            </div>
        </div>
                   
        <ul id="neutron-multi-image-upload-container-{{ id }}" class="neutron-sortable">
            {% for key, child in form %}
                <li data-index="{{ key }}">
                    {{ form_widget(child) }}
                    <img src="{{ asset(configs.dir~form[key].name.get('value')) }}" 
                        width="{{ configs.minWidth }}" height="{{ configs.minHeight }}" />
                </li>
            {% endfor %}
        </ul>
            
        <div id="neutron-progressbar-prototype-{{ id }}" style="display:none">
            <div id="__id__"  class="neutron-progressbar-multi-image" style="width:350px; height:10px;" ></div>
            <div class="neutron-progress-info">
                <div class="ui-widget">
                    <div class="ui-state-highlight ui-corner-all" style="width:340px;padding:5px;"> 
                        <span style="float:right">
                            <a href="#remove" id="neutron-multi-upload-remove-image-__id__">
                                <img src="{{ asset('bundles/neutronform/images/cancel.png') }}" />
                            </a>
                        </span>
                        <span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                        <span>{% trans from translation_domain %}upload_progress{% endtrans %}: <strong>0%</strong></span>
                        <br/>
                        <span>__image_name__ (__image_size__)</span>
                    </div>
                </div>
            </div>
        </div>
                
        <span data-prototype="{{ form_widget(form.get('prototype')) | e }}" data-options="{{ configs|merge({'base_url': asset('')})|json_encode }}"
            class="neutron-multi-image-upload" style="display:none"></span>
        
        <div style="clear:both;"></div> 
        {{ block('neutron_image_controls') }}
        {{ block('neutron_image_upload_crop') }}
        {{ block('neutron_upload_meta') }}
    </div>
</div>
{% endspaceless %}
{% endblock neutron_multi_image_upload_collection_widget %}

{% block neutron_file_upload_widget %}
{% spaceless %}
<div>
    {{ form_widget(form) }} 
    <span class="neutron-file-upload" data-options="{{ configs|json_encode}}" style="display:none"></span>
    
    <span class="neutron-file-upload-main" style="display:none">
        <div id="neutron-file-error-{{ id }}" class="ui-state-error" 
             style="width:300px; padding:10px; margin-bottom: 5px; display:none">
            <span style="float:right">
                <a href="#close" id="neutron-upload-error-cancel-{{ id }}">
                    <img src="{{ asset('bundles/neutronform/images/cancel.png') }}" />
                </a>
            </span>
            <p>
                <span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span> 
                <span class="neutron-fileupload-error">&nbsp</span>
        	</p>
        </div>
                    
        <div  id="neutron-progressbar-{{ id }}" style="width:350px; height:10px; display: none" ></div>
       
        <div class="neutron-progress-info" style="display:none">
            <div class="ui-widget">
                <div class="ui-state-highlight ui-corner-all" style="width:340px;padding:5px;"> 
                    <span style="float:right" id="neutron-upload-remove-file-{{id}}">
                        <a href="#remove">
                            <img src="{{ asset('bundles/neutronform/images/cancel.png') }}" />
                        </a>
                    </span>
                    <span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                    <span>{% trans from translation_domain %}upload_progress{% endtrans %}: <strong>0%</strong></span>
                    <br/>
                    <span id="neutron-file-info-{{ id }}">__file_name__ (__file_size__)</span>
                </div>
            </div>
        </div>
                    
        <div id="neutron-upload-file-{{ id }}" class="ui-state-highlight ui-corner-all" style="width:440px;padding:5px;"> 
            
            <p id="neutron-file-empty-{{ id }}" {% if form.name.get('value') %}style="display:none"{% endif %}>
                <span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                {% trans from translation_domain %}no_file_uploaded{% endtrans %}
            </p>
            
            <p id="neutron-file-name-{{ id }}" {% if form.originalName.get('value') is empty %}style="display:none"{% endif %}>
                <span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                <strong>{% trans from translation_domain %}filename{% endtrans %}: </strong>
                <span class="neutron-file-name">{{ form.originalName.get('value') }}</span>
            </p>
    
        </div>
        
        {{ block('neutron_file_upload_controls') }}
        {{ block('neutron_upload_meta') }}  
    </span>
</div>

{% endspaceless %}
{% endblock neutron_file_upload_widget %}

{% block neutron_file_upload_controls %}
{% spaceless %}
<div class="neutron-controls">
    <button id="neutron-file-btn-upload-{{ id }}" class="ui-icon ui-icon-newwin neutron-file-upload-button" 
        title="{% trans from translation_domain %}button_upload_title{% endtrans %}"></button> &nbsp;
    {% if configs.file_options.enabled_button %}
        <a id="neutron-file-btn-enabled-{{ id }}" 
        class="ui-icon {% if (configs.enabled_value)  %} ui-icon-bullet {% else %} ui-icon-radio-on {% endif %} neutron-file-upload-button" 
            title="{% trans from translation_domain %}button_title_enabled{% endtrans %}"></a>  &nbsp;
    {% endif %}
    {% if configs.file_options.meta_button %}
        <a id="neutron-meta-btn-edit-{{ id }}" class="ui-icon ui-icon-pencil neutron-file-upload-button" 
            title="{% trans from translation_domain %}button_edit_meta_title{% endtrans %}"></a>  &nbsp;
    {% endif %}
    <a id="neutron-file-btn-remove-{{ id }}" class="ui-icon ui-icon-trash neutron-file-upload-button" 
        title="{% trans from translation_domain %}button_remove_title{% endtrans %}"></a>  &nbsp;
</div>
{% endspaceless %}
{% endblock neutron_file_upload_controls %}

