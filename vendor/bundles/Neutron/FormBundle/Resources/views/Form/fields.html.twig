            <div class="ui-state-highlight" style="width:340px;height:70px;padding:5px;"> 
                <p>
                    <span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                    <strong>{% trans from translation_domain %}filename{% endtrans %}: </strong>
                    <span class="neutron-file-name">__name__</span>
                </p>
            </div>
        </div>
    	
        <div id="neutron-progressbar-prototype-{{ id }}" style="display:none">
            <div id="__id__"  class="neutron-progressbar-multi-file" style="width:350px; height:10px;" ></div>
            <div class="neutron-progress-info">
                <div class="ui-widget">
                    <div class="ui-state-highlight ui-corner-all" style="width:340px;padding:5px;"> 
                        <span style="float:right">
                            <a href="#remove" id="neutron-multi-upload-remove-file-__id__">
                                <img src="{{ asset('bundles/neutronform/images/cancel.png') }}" />
                            </a>
                        </span>
                        <span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                        <span>{% trans from translation_domain %}upload_progress{% endtrans %}: <strong>0%</strong></span>
                        <br/>
                        <span>__file_name__ (__file_size__)</span>
                    </div>
                </div>
            </div>
        </div>
        
        <span data-prototype="{{ form_widget(form.get('prototype')) | e }}" data-options="{{ configs|json_encode}}"
            class="neutron-multi-file-upload" style="display:none"></span>
        <div style="clear:both;"></div> 
        
        {{ block('neutron_file_upload_controls') }}
        {{ block('neutron_upload_meta') }}
    </div>
</div>

{% endspaceless %}
{% endblock neutron_multi_file_upload_collection_widget %}

