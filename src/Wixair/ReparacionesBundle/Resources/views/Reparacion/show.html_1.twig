{% extends "WixairReparacionesBundle::layout.html.twig" %}
{%block wrapper%}
<h1>Wixair</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Fecha_entrada</th>
            <td>{{ entity.fechaentrada|date('Y-m-d') }}</td>
        </tr>
        <tr>
            <th>Fecha_salida</th>
            <td>{{ entity.fechasalida|date('Y-m-d') }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Tlf</th>
            <td>{{ entity.tlf }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Equipo</th>
            <td>{{ entity.tipoequipo }}</td>
        </tr>
        <tr>
            <th>Numero_serie</th>
            <td>{{ entity.numeroserie }}</td>
        </tr>
        <tr>
            <th>Accesorios</th>
            <td>{{ entity.accesorios }}</td>
        </tr>
        <tr>
            <th>Averia</th>
            <td>{{ entity.averia }}</td>
        </tr>
        <tr>
            <th>Reparacion</th>
            <td>{{ entity.reparacion }}</td>
        </tr>
        <tr>
            <th>Precio</th>
            <td>{{ entity.precio }}</td>
        </tr>
        <tr>
            <th>Presupuesto_aceptado</th>
            <td>{{ entity.presupuestoaceptado }}</td>
        </tr>
        <tr>
            <th>Observaciones</th>
            <td>{{ entity.observaciones }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('reparacion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reparacion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('reparacion_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{%endblock%}